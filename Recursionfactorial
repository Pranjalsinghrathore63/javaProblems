package Recursion;

public class Recursion1 {

	public static int calcfactorial(int n){
		
		if(n == 1 ||  n== 0) {
			return 1;
			
		}
		int fn1 = calcfactorial(n-1);
		int fn = n * fn1;
	    return fn;
		
	}
	public static void main(String[] args) {

		int n = 6;
		int ans = calcfactorial(n);
		System.out.println(ans);
	
	}

}
